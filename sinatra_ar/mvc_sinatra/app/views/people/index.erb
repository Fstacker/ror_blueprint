<h1>People</h1>
<thead>Name</thead>
<thead>Birthdate</thead>
<% end %>
<hr>

<tbody>
	<thead>Edit</thead>
	<% @people.each do |person| %>
	<tr>
		<td>
			<a href="<%= "/people/#{person.id}"%>" > <%= "#{person.first_name} #{person.last_name}"%>Table Data </a>
		</td>

		<td>
			<%= "#{person.birthdate}" %>
		</td>

		<td>
			<a href="<%= "/people/#{person.id}"%>" >Edit</a>
		</td>
	</tr>
</tbody>

